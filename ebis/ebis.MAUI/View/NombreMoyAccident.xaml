<?xml version="1.0" encoding="utf-8" ?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ebis.MAUI.NbrMoyAccident.NombreMoyenAccidentPage"
             xmlns:vm="clr-namespace:ebis.MAUI.ViewModel"
             x:DataType="vm:NbrAccidentViewModel"
             xmlns:model="clr-namespace:ebis.MAUI.Model"
             Title="Nombre moyen d'accidents sur les 5 dernières années">

    <StackLayout>
        <ListView ItemsSource="{Binding Resultats}">
            <ListView.ItemTemplate>
                <DataTemplate x:DataType="model:Element">
                    <TextCell Text="{Binding .}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>

</ContentPage>