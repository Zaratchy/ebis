<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ebis.MAUI.JournalEntretien.JournalEntretienPage"
             xmlns:vm="clr-namespace:ebis.MAUI.ViewModel"
             x:DataType="vm:JournalEntretienViewModel"
             Title="Journal d'entretiens">

    <StackLayout>
        <ListView ItemsSource="{Binding Resultats}">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding .}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>

</ContentPage>